{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Spraycoater (PyQt6, XLaunch)",
      "type": "debugpy",
      "request": "launch",
      "program": "/root/Spraycoater/src/app/main_gui.py",
      "console": "integratedTerminal",
      "python": "/root/Spraycoater/.venv/bin/python",
      "cwd": "/root/Spraycoater",
      "env": {
        "PYTHONPATH": "/root/Spraycoater/src",
        "SAFE_NO_PREVIEW": "0",
        "QT_OPENGL": "software",
        "QT_X11_NO_MITSHM": "1"
      }
    }
  ]
}
