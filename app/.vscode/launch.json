{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run GUI App",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/source/app/main_gui.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONUNBUFFERED": "1",
                "RMW_IMPLEMENTATION": "rmw_fastrtps_cpp",
                "XDG_RUNTIME_DIR": "/tmp/runtime-root",
                "ROS_HOME": "/tmp/ros"
            },
            "justMyCode": false
        }
    ]
}
